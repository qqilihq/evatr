{
  "name": "evatr",
  "description": "Validate foreign VAT numbers via German “Bundeszentralamt für Steuern”",
  "author": "Philipp Katz <philipp@philippkatz.de>",
  "version": "6.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qqilihq/evatr/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qqilihq/evatr.git"
  },
  "keywords": [
    "vat",
    "validation",
    "germany",
    "Bundeszentralamt für Steuern",
    "typescript"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint --ext .ts ./lib ./test",
    "build": "tsc -p tsconfig.build.json",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf ./.nyc_output ./coverage ./dist",
    "preversion": "run-s clean lint test build",
    "scrape-error-codes": "ts-node ./dev/scrape-error-codes.ts"
  },
  "dependencies": {
    "fast-xml-parser": "^4.1.4",
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/node": "^10",
    "@types/superagent": "4.1.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vitest/coverage-v8": "^0.32.2",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "expect.js": "^0.3.1",
    "moment-timezone": "^0.5.33",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "14.21.3",
    "yarn": "1.22.19"
  }
